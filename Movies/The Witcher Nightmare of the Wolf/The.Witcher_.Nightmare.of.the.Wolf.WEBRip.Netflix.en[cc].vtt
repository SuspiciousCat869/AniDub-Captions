<c.bg_transparent>-[yelps]</c.bg_transparent>
<c.bg_transparent>-[creature snarling]</c.bg_transparent>

485
00:29:29.083 --> 00:29:30.333 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[boy groans]</c.bg_transparent>

486
00:29:31.000 --> 00:29:31.833 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[sighs]</c.bg_transparent>

487
00:29:34.750 --> 00:29:35.750 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>This way.</c.bg_transparent>

488
00:29:40.416 --> 00:29:42.541 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[creature snarling]</c.bg_transparent>

489
00:29:47.625 --> 00:29:49.583 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[high-pitched ringing]</c.bg_transparent>

490
00:29:50.250 --> 00:29:52.166 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[boys screaming]</c.bg_transparent>

491
00:30:06.291 --> 00:30:07.125 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>Uh…</c.bg_transparent>

492
00:30:11.000 --> 00:30:12.500 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[Vesemir screaming]</c.bg_transparent>

493
00:30:15.083 --> 00:30:16.166 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[gasps]</c.bg_transparent>

494
00:30:16.833 --> 00:30:17.666 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[groans]</c.bg_transparent>

495
00:30:19.000 --> 00:30:19.833 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>Oh…</c.bg_transparent>

496
00:30:20.958 --> 00:30:22.000 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[sighs]</c.bg_transparent>

497
00:30:31.666 --> 00:30:32.500 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[gasps]</c.bg_transparent>

498
00:30:34.083 --> 00:30:35.416 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[shudders]</c.bg_transparent>

499
00:30:36.666 --> 00:30:38.666 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[somber music playing]</c.bg_transparent>

500
00:30:53.500 --> 00:30:54.583 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[sobbing]</c.bg_transparent>

