01:06:33.958 --> 01:06:35.958 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[yelling]</c.bg_transparent>

1026
01:06:43.750 --> 01:06:44.750 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[grunts]</c.bg_transparent>

1027
01:06:51.458 --> 01:06:52.291 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[yelling]</c.bg_transparent>

1028
01:06:53.541 --> 01:06:55.666 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[dramatic music playing]</c.bg_transparent>

1029
01:07:13.541 --> 01:07:14.833 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[yells]</c.bg_transparent>

1030
01:07:35.916 --> 01:07:37.083 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[yells]</c.bg_transparent>

