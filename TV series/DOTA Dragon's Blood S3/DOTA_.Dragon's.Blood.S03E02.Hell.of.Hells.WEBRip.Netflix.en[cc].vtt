351
00:22:20.750 --> 00:22:24.833 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[screaming]</c.bg_transparent>

352
00:22:31.333 --> 00:22:34.583 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[shattering]</c.bg_transparent>

353
00:22:36.333 --> 00:22:38.541 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[explosions]</c.bg_transparent>

354
00:22:50.166 --> 00:22:51.833 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[Sagan growls]</c.bg_transparent>

355
00:22:53.708 --> 00:22:54.708 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>[Mirana] Davion.</c.bg_transparent>

356
00:22:56.708 --> 00:22:57.708 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>Davion.</c.bg_transparent>

357
00:22:58.375 --> 00:23:01.625 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>You look terrible. Were you in a fight?</c.bg_transparent>

358
00:23:01.708 --> 00:23:02.791 position:50.00%,middle align:middle size:80.00% line:79.33% 
<c.bg_transparent>[chuckles]</c.bg_transparent>
<c.bg_transparent>Yes.</c.bg_transparent>

359
00:23:04.000 --> 00:23:06.000 position:50.00%,middle align:middle size:80.00% line:84.67% 
<c.bg_transparent>We won. And no thanks to you.</c.bg_transparent>

